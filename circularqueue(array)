#include<stdio.h>
#include<stdlib.h>
#define n 5
int f=0,r=0,c=0;
int q[n];
void enqueue()
{
    int ch;
    if(c==n)
    {
        printf("queue is filled\n");
    }
    else
    {
        printf("enter value into queue\n");
        scanf("%d",&ch);
        q[r]=ch;
        r=r+1;
        c=c+1;
    }
}
void dequeue()
{
    if(f==r)
    {
        printf("queue is empty\n");
    }
    else
    {
        printf("dequeued element is %d\n",q[f]);
        f=f+1;
        c=c-1;
    }
}
void display()
{
    int i=0;
    for(i=0;i<c;i++)
    {
        printf("%d\n",q[i]);
    }
}
void main()
{
    int s;
    while(1)
    {
        printf("enter char into switch(as a num)\n");
        scanf("%d",&s);
        switch(s)
        {
            case 1:enqueue();
                   break;
            case 2:dequeue();
                   break;
            case 3:display();
                    break;
            default:exit;        
        }
    }
}

